{% extends '::layout.html.twig' %}

{% block content %}
<table border="0" cellspacing="5" cellpadding="5" class="display table" width="100%"
    data-toggle="datatable">
    <thead>
        <tr>
            <th>{{ 'username' | trans }}</th>
            <th>{{ 'email' | trans }}</th>
        </tr>
        <tbody>
           {% for user in users %}
           <tr>
               <td>{{ user.username }}</td>
               <td>{{ user.email}}</td>
           </tr>
           {% endfor %}
        </tbody>
    </thead>
</table>
{% endblock %}
